	<sql id="selectVo">
        select *
        from dev_info r
    </sql>	
	
	<!-- 查询条件 -->
	<sql id="sqlwhereSearch">
		<where>
			<if test=" id !=null">
				and id = #{id}
			</if>
			<if test=" dname != null and dname != ''">
				AND dname  like concat(concat('%',#{dname}),'%')
			</if>
			<if test=" dnumber !=null and dnumber != ''">
				and dnumber = #{dnumber}
			</if>
			<if test=" dalias !=null and dalias != ''">
				AND dalias  like concat(concat('%',#{dalias}),'%')
			</if>
		</where>
	</sql>
	

    
    <select id="select" parameterType="DevInfo" resultMap="result">
        <include refid="selectVo"/>
        <include refid="sqlwhereSearch"/>
    </select>
    
    <select id="selectList" parameterType="DevInfo" resultMap="result">
        <include refid="selectVo"/>
        where  1 = 1
        <if test=" id != null and id != 0">
			AND id = #{id}
		</if>
		<if test=" dname != null and dname != ''">
				AND dname  like concat(concat('%',#{dname}),'%')
		</if>
		<if test=" dnumber !=null and dnumber != ''">
			and dnumber = #{dnumber}
		</if>
		<if test=" dalias !=null and dalias != ''">
			AND dalias  like concat(concat('%',#{dalias}),'%')
		</if>
		
		order by r.id
    </select>
      <select id="selectByName" parameterType="Long" resultMap="result">
		<include refid="selectVo"/>
		where  1 = 1
		<if test=" dname != null and dname != ''">
				AND dname  like concat(concat('%',#{dname}),'%')
		</if>
	</select>
	
	<select id="selectById" parameterType="Long" resultMap="result">
		<include refid="selectVo"/>
		where r.id = #{id}
	</select>
	
    <delete id="deleteById" parameterType="Long">
		delete from dev_info where id = #{id}
	</delete>
	 <delete id="deleteByIds" parameterType="Long">
 		delete from dev_info where id in
 		<foreach collection="array" item="id" open="(" separator="," close=")">
 			#{id}
        </foreach> 
 	</delete>
    
