	<!-- 查询条件 -->
	<sql id="sqlwhereSearch">
		<where>
		<if test=" prj_no != null and prj_no != '' ">
			AND "Prj_No" = #{prj_no}
		</if>
		<if test=" areaid !=null">
				and AREAID = #{areaid}
		</if>
		<if test=" areaname != null and areaname != ''">
			AND AREANAME  like concat(concat('%',#{areaname}),'%')
		</if>
		<if test=" prj_name != null and prj_name != ''">
				AND "Prj_Name"  like concat(concat('%',#{prj_name}),'%')
		</if>
		<if test=" exp_no !=null and exp_no != ''">
			and "Exp_No" = #{exp_no}
		</if>
		</where>
	</sql>
	

    
    <select id="select" parameterType="PS_POINT" resultMap="result">
        <include refid="selectVo"/>
        <include refid="sqlwhereSearch"/>
    </select>
    
    <select id="selectList" parameterType="PS_POINT" resultMap="result">
        <include refid="selectVo"/>
        where  1 = 1
