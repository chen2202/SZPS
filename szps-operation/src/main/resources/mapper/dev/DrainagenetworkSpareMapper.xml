			<if test=" devname != null and devname != ''">
				AND devname  like concat(concat('%',#{devname}),'%')
			</if>
			<if test=" dnumber !=null and dnumber != ''">
				and dnumber = #{dnumber}
			</if>
			<if test=" supplier !=null and supplier != ''">
