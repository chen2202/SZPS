<if test=" deptid != null and deptid != ''">deptid = #{deptid},</if>
<if test=" deptname != null and deptname != ''">deptname = #{deptname},</if>
<if test=" dlocation != null and dlocation != ''">dlocation = #{dlocation},</if>
<if test=" assetname != null and assetname != ''">assetname = #{assetname},</if>
<if test=" startingplace != null and startingplace != ''">startingplace = #{startingplace},</if>
<if test=" endplace != null and endplace != ''">endplace = #{endplace},</if>
<if test=" dlength != null and dlength != ''">dlength = #{dlength},</if>
<if test=" ddepth != null and ddepth != ''">ddepth = #{ddepth},</if>
<if test=" dhigh != null and dhigh != ''">dhigh = #{dhigh},</if>
<if test=" material != null and material != ''">material = #{material},</if>
<if test=" insulation != null and insulation != ''">insulation = #{insulation},</if>
<if test=" layway != null and layway != ''">layway = #{layway},</if>
<if test=" aczoiling != null and aczoiling != ''">aczoiling = #{aczoiling},</if>
<if test=" heatinsulatingmaterial != null and heatinsulatingmaterial != ''">heatinsulatingmaterial = #{heatinsulatingmaterial},</if>
<if test=" builttime != null and builttime != ''">builttime = #{builttime},</if>
<if test=" dstatus != null and dstatus != ''">dstatus = #{dstatus},</if>
<if test=" originalvalue != null and originalvalue != ''">originalvalue = #{originalvalue},</if>


			
			<if test=" remark != null and remark != ''">remark = #{remark},</if>
 			<if test=" updateBy != null and updateBy != ''">update_by = #{updateBy},</if>
 			update_time = sysdate
 		</set>
 		where id = #{id}
	</update>
	  
    <insert id="insert" parameterType="DevInfo">
