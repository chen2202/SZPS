<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.szps.web.mapper.dev.fixedasset..DevInfoMapper">
 
	<resultMap type="DevInfo" id="result">
		<id     property="id"    column="id"    />
<result     property="dlocation"   column="dlocation"   />
<result     property="assetname"   column="assetname"   />
<result     property="height"   column="height"   />
<result     property="dbh"   column="dbh"   />
<result     property="crowndiameter"   column="crowndiameter"   />
<result     property="measurement"   column="measurement"   />
<result     property="realquantity"   column="realquantity"   />
<result     property="originalvalue"   column="originalvalue"   />



        <result property="createBy"      column="create_by"      />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"      column="update_by"      />
        <result property="updateTime"    column="update_time"    />
		<result property="remark"    column="remark"    />
    </resultMap> 

	<sql id="selectVo">
        select *
        from dev_info r
    </sql>	
	
	<!-- 查询条件 -->
	<sql id="sqlwhereSearch">
		<where>
			<if test=" id !=null">
				and id = #{id}
			</if>
			<if test=" dname != null and dname != ''">
				AND dname  like concat(concat('%',#{dname}),'%')
			</if>
			<if test=" dnumber !=null and dnumber != ''">
				and dnumber = #{dnumber}
			</if>
			<if test=" dalias !=null and dalias != ''">
				AND dalias  like concat(concat('%',#{dalias}),'%')
			</if>
		</where>
	</sql>
	

    
    <select id="select" parameterType="DevInfo" resultMap="result">
        <include refid="selectVo"/>
        <include refid="sqlwhereSearch"/>
    </select>
    
    <select id="selectList" parameterType="DevInfo" resultMap="result">
        <include refid="selectVo"/>
        where  1 = 1
        <if test=" id != null and id != 0">
			AND id = #{id}
		</if>
		<if test=" dname != null and dname != ''">
				AND dname  like concat(concat('%',#{dname}),'%')
		</if>
		<if test=" dnumber !=null and dnumber != ''">
			and dnumber = #{dnumber}
		</if>
		<if test=" dalias !=null and dalias != ''">
			AND dalias  like concat(concat('%',#{dalias}),'%')
		</if>
		
		order by r.id
    </select>
      <select id="selectByName" parameterType="Long" resultMap="result">
		<include refid="selectVo"/>
		where  1 = 1
		<if test=" dname != null and dname != ''">
				AND dname  like concat(concat('%',#{dname}),'%')
		</if>
	</select>
	
	<select id="selectById" parameterType="Long" resultMap="result">
		<include refid="selectVo"/>
		where r.id = #{id}
	</select>
	
    <delete id="deleteById" parameterType="Long">
		delete from dev_info where id = #{id}
	</delete>
	 <delete id="deleteByIds" parameterType="Long">
 		delete from dev_info where id in
 		<foreach collection="array" item="id" open="(" separator="," close=")">
 			#{id}
        </foreach> 
 	</delete>
    
    <update id="update" parameterType="DevInfo">
 		update dev_info
 		<set>
<if test=" dlocation != null and dlocation != ''">dlocation = #{dlocation},</if>
<if test=" assetname != null and assetname != ''">assetname = #{assetname},</if>
<if test=" height != null and height != ''">height = #{height},</if>
<if test=" dbh != null and dbh != ''">dbh = #{dbh},</if>
<if test=" crowndiameter != null and crowndiameter != ''">crowndiameter = #{crowndiameter},</if>
<if test=" measurement != null and measurement != ''">measurement = #{measurement},</if>
<if test=" realquantity != null and realquantity != ''">realquantity = #{realquantity},</if>
<if test=" originalvalue != null and originalvalue != ''">originalvalue = #{originalvalue},</if>


			
			<if test=" remark != null and remark != ''">remark = #{remark},</if>
 			<if test=" updateBy != null and updateBy != ''">update_by = #{updateBy},</if>
 			update_time = sysdate
 		</set>
 		where id = #{id}
	</update>
	  
    <insert id="insert" parameterType="DevInfo">
    	 <selectKey keyProperty="id" resultType="long" order="BEFORE">
			SELECT seq_dev_info.NEXTVAL as id FROM DUAL
		</selectKey>
    
		INSERT INTO dev_info(
			<if test=" id != null and id!= 0">id,</if>
<if test=" dlocation != null and  dlocation != ''">dlocation,</if>
<if test=" assetname != null and  assetname != ''">assetname,</if>
<if test=" height != null and  height != ''">height,</if>
<if test=" dbh != null and  dbh != ''">dbh,</if>
<if test=" crowndiameter != null and  crowndiameter != ''">crowndiameter,</if>
<if test=" measurement != null and  measurement != ''">measurement,</if>
<if test=" realquantity != null and  realquantity != ''">realquantity,</if>
<if test=" originalvalue != null and  originalvalue != ''">originalvalue,</if>


			
			<if test=" remark != null and remark != ''">REMARK,</if>
			<if test=" createBy != null and createBy != ''">create_by,</if>
			create_time
		) VALUES (
			<if test=" id != null and id!= 0">#{id},</if>
<if test=" dlocation != null and dlocation != ''">#{dlocation},</if>
<if test=" assetname != null and assetname != ''">#{assetname},</if>
<if test=" height != null and height != ''">#{height},</if>
<if test=" dbh != null and dbh != ''">#{dbh},</if>
<if test=" crowndiameter != null and crowndiameter != ''">#{crowndiameter},</if>
<if test=" measurement != null and measurement != ''">#{measurement},</if>
<if test=" realquantity != null and realquantity != ''">#{realquantity},</if>
<if test=" originalvalue != null and originalvalue != ''">#{originalvalue},</if>


			
			<if test=" remark != null and remark != ''">#{remark},</if>
			<if test=" createBy != null and createBy != ''">#{createBy},</if>
			sysdate			
		)
	</insert>
    
</mapper>