			<if test=" origDeptOpinion != null and  origDeptOpinion != ''">origDeptOpinion,</if>
			<if test=" origPeople != null and  origPeople != ''">origPeople,</if>
			<if test=" equipDeptOpinion != null and  equipDeptOpinion != ''">equipDeptOpinion,</if>
