			<if test=" tPart != null and tPart != ''">#{tPart},</if>
			<if test=" accessory != null and accessory != ''">#{accessory},</if>
			<if test=" applyReason != null and applyReason != ''">#{applyReason},</if>
			<if test=" applicant != null and applicant != ''">#{applicant},</if>
			<if test=" origDeptOpinion != null and origDeptOpinion != ''">#{origDeptOpinion},</if>
			<if test=" origPeople != null and origPeople != ''">#{origPeople},</if>
