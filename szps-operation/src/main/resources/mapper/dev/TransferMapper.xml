<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.szps.web.mapper.dev.TransferMapper">
 
	<resultMap type="Transfer" id="result">
		<id     property="id"    column="id"    />
		<result     property="deptId"   column="deptId"   />
		<result     property="applyDate"   column="applyDate"   />
		<result     property="applyDept"   column="applyDept"   />
		<result     property="applyEquip"   column="applyEquip"   />
		<result     property="devModel"   column="devModel"   />
		<result     property="origDept"   column="origDept"   />
		<result     property="devStatu"   column="devStatu"   />
		<result     property="devNumber"   column="devNumber"   />
		<result     property="transferType"   column="transferType"   />
		<result     property="origValue"   column="origValue"   />
		<result     property="devType"   column="devType"   />
		<result     property="planoutDate"   column="planoutDate"   />
		<result     property="depreciateYear"   column="depreciateYear"   />
		<result     property="supplier"   column="supplier"   />
		<result     property="planReturnDate"   column="planReturnDate"   />
		<result     property="dutyCost"   column="dutyCost"   />
		<result     property="buyDate"   column="buyDate"   />
		<result     property="tPart"   column="tPart"   />
		<result     property="accessory"   column="accessory"   />
		<result     property="applyReason"   column="applyReason"   />
		<result     property="applicant"   column="applicant"   />
		<result     property="origDeptOpinion"   column="origDeptOpinion"   />
		<result     property="origPeople"   column="origPeople"   />
		<result     property="equipDeptOpinion"   column="equipDeptOpinion"   />
		<result     property="equipPeople"   column="equipPeople"   />
		<result     property="leaderOpinion"   column="leaderOpinion"   />
		<result     property="leader"   column="leader"   />

        <result property="createBy"      column="create_by"      />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"      column="update_by"      />
        <result property="updateTime"    column="update_time"    />
		<result property="remark"    column="remark"    />
		<result property="delFlag"    column="delFlag"    />
		<result     property="dept_id"   column="dept_id"   />		
    </resultMap> 

	<sql id="selectVo">
        select *
        from dev_transfer r
    </sql>	
	
	<!-- 查询条件 -->
	<sql id="sqlwhereSearch">
		<where>
			<if test=" id !=null">
				and id = #{id}
			</if>
			<if test=" applyDate !=null and applyDate != ''">
				and applyDate = #{applyDate}
			</if>
			<if test=" applyDept !=null and applyDept != ''">
				and applyDept = #{applyDept}
			</if>
			<!-- <if test=" deptId !=null and deptId != ''">
				and deptId = #{deptId}
			</if> -->
		</where>
	</sql>
	

    
    <select id="select" parameterType="Transfer" resultMap="result">
        <include refid="selectVo"/>
        <include refid="sqlwhereSearch"/>
    </select>
    
    <select id="selectList" parameterType="Transfer" resultMap="result">
        <include refid="selectVo"/>
        where  delFlag = '1'
        <!-- <if test=" deptId != null and deptId != 0">
			AND deptId = #{deptId}
		</if> -->
		<if test=" applyDate != null and applyDate != ''">
			AND applyDate = #{applyDate}
		</if>
		<if test=" applyDept !=null and applyDept != ''">
				and applyDept = #{applyDept}
			</if>
		<!-- 数据范围过滤 -->
		${params.dataScope}	
		order by r.id, r.applyDate
    </select>
      <select id="selectById" parameterType="Long" resultMap="result">
		<include refid="selectVo"/>
		where r.id = #{id}
	</select>
	
    <delete id="deleteById" parameterType="Long">
		update dev_transfer set DELFLAG = '2' where id = #{id}
	</delete>
	 <delete id="deleteByIds" parameterType="Long">
 		delete from dev_transfer where id in
 		<foreach collection="array" item="id" open="(" separator="," close=")">
 			#{id}
        </foreach> 
 	</delete>
    
    <update id="update" parameterType="Transfer">
 		update dev_transfer
 		<set>
			<if test=" deptId != null and deptId != ''">deptId = #{deptId},</if>
			<if test=" applyDate != null and applyDate != ''">applyDate = #{applyDate},</if>
			<if test=" applyDept != null and applyDept != ''">applyDept = #{applyDept},</if>
			<if test=" applyEquip != null and applyEquip != ''">applyEquip = #{applyEquip},</if>
			<if test=" devModel != null and devModel != ''">devModel = #{devModel},</if>
			<if test=" origDept != null and origDept != ''">origDept = #{origDept},</if>
			<if test=" devStatu != null and devStatu != ''">devStatu = #{devStatu},</if>
			<if test=" devNumber != null and devNumber != ''">devNumber = #{devNumber},</if>
			<if test=" transferType != null and transferType != ''">transferType = #{transferType},</if>
			<if test=" origValue != null and origValue != ''">origValue = #{origValue},</if>
			<if test=" devType != null and devType != ''">devType = #{devType},</if>
			<if test=" planoutDate != null and planoutDate != ''">planoutDate = #{planoutDate},</if>
			<if test=" depreciateYear != null and depreciateYear != ''">depreciateYear = #{depreciateYear},</if>
			<if test=" supplier != null and supplier != ''">supplier = #{supplier},</if>
			<if test=" planReturnDate != null and planReturnDate != ''">planReturnDate = #{planReturnDate},</if>
			<if test=" dutyCost != null and dutyCost != ''">dutyCost = #{dutyCost},</if>
			<if test=" buyDate != null and buyDate != ''">buyDate = #{buyDate},</if>
			<if test=" tPart != null and tPart != ''">tPart = #{tPart},</if>
			<if test=" accessory != null and accessory != ''">accessory = #{accessory},</if>
			<if test=" applyReason != null and applyReason != ''">applyReason = #{applyReason},</if>
			<if test=" applicant != null and applicant != ''">applicant = #{applicant},</if>
			<if test=" origDeptOpinion != null and origDeptOpinion != ''">origDeptOpinion = #{origDeptOpinion},</if>
			<if test=" origPeople != null and origPeople != ''">origPeople = #{origPeople},</if>
			<if test=" equipDeptOpinion != null and equipDeptOpinion != ''">equipDeptOpinion = #{equipDeptOpinion},</if>
			<if test=" equipPeople != null and equipPeople != ''">equipPeople = #{equipPeople},</if>
			<if test=" leaderOpinion != null and leaderOpinion != ''">leaderOpinion = #{leaderOpinion},</if>
			<if test=" leader != null and leader != ''">leader = #{leader},</if>
			
			<if test=" delFlag != null and delFlag!= ''">delFlag = #{delFlag},</if>
			<if test=" remark != null and remark != ''">remark = #{remark},</if>
 			<if test=" updateBy != null and updateBy != ''">update_by = #{updateBy},</if>
 			update_time = sysdate
 		</set>
 		where id = #{id}
	</update>
	  
    <insert id="insert" parameterType="Transfer">
    	 <selectKey keyProperty="id" resultType="long" order="BEFORE">
			SELECT seq_dev_transfer.NEXTVAL as id FROM DUAL
		</selectKey>
    
