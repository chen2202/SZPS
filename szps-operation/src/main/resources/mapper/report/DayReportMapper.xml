    <insert id="insertReport" parameterType="DayReport">
    	 <selectKey keyProperty="id" resultType="long" order="BEFORE">
			SELECT seq_report_day.NEXTVAL as id FROM DUAL
		</selectKey>
    
		INSERT INTO report_day(
				<if test=" id != null and id!= 0">id,</if>
				<if test=" deptId != null">deptId,</if>
				<if test=" deptName != null">deptName,</if>
				<if test=" reportDate != null">reportDate,</if>
				<if test=" weather != null">weather,</if>
				<if test=" temperature != null">temperature,</if>
				<if test=" humidity != null">humidity,</if>
				<if test=" earlyWorker != null">earlyWorker,</if>
				<if test=" middleWorker != null">middleWorker,</if>
				<if test=" lastWorker != null">lastWorker,</if>
				<if test=" dwt1 != null">dwt1,</if>
				<if test=" dwt2 != null">dwt2,</if>
