	<sql id="selectReportVo">
        select * from report_day r
    </sql>	
	
	<!-- 查询条件 -->
	<sql id="sqlwhereSearch">
		<where>
			<if test="id !=null">
				and id = #{id}
			</if>
			<if test="reportDate !=null and reportDate != ''">
				and reportDate = #{reportDate}
			</if>
		</where>
	</sql>
	

    
    <select id="selectReport" parameterType="DayReport" resultMap="ReportResult">
        <include refid="selectReportVo"/>
        <include refid="sqlwhereSearch"/>
    </select>
    
    <select id="selectReportList" parameterType="DayReport" resultMap="ReportResult">
        <include refid="selectReportVo"/>
         where delFlag = '1'
        <if test="deptId != null and deptId != 0">
			AND deptId = #{deptId}
		</if>
		<if test="reportDate != null and reportDate != ''">
			AND reportDate = #{reportDate}
		</if>
		<!-- 数据范围过滤 -->
		${params.dataScope}	
		order by r.id, r.reportDate
    </select>
    
    
    <select id="selectReportById" parameterType="Long" resultMap="ReportResult">
		<include refid="selectReportVo"/>
		where r.id = #{id}
	</select>
	
    <delete id="deleteReportById" parameterType="Long">
		update report_day set delFlag = '2' where id = #{id}
	</delete>
	 <delete id="deleteReportByIds" parameterType="Long">
 		delete from report_day where id in
 		<foreach collection="array" item="id" open="(" separator="," close=")">
 			#{id}
        </foreach> 
 	</delete>
 	
